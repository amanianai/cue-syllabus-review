<!-- EXAMPLE: Canvas -->
<!-- Please, note. This example is just to show how you can combine your <canvas> element as a background. -->
<!-- 1) Copy the script in the head -->
<script src="https://cdnjs.cloudflare.com/ajax/libs/d3/3.5.5/d3.min.js" charset="utf-8"></script>

<!-- 2) Add id="cover" on the "background" element -->
<section id="intro" class="slide whiteSlide">
  <div class="content disable-pointer-events">
    <div class="container">
      <div class="wrap">

        <div class="fix-10-12">
          <h1>Canvas Example</h1>
        </div>

      </div>
    </div>
  </div>
  <div id="cover" class="background"></div>
</section>

<!-- 3) Copy the script in the head our custom.js file -->
