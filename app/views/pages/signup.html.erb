<body class="slides zoom horizontal noPreload animated">
  <section class="slide fade-1 kenBurns slideWhite">
    <div class="content">
      <div class="container">
        <div class="wrap ">
          <!-- <div class="eqmd-bg" style="z-index:">eqmd</div> -->
          <ul class="fix-12-12">
            <h1 class="huge ae-1">Data Matters.</h1>
            <p class="hero ae-2">
              Please help us accurately capture
              <a href="#"><u>anoynmous</u></a> <br/> details about who is
              using our tools.
            </p>
            <div class="form-42 margin-top-5">
              <div class="selected left ae-4">
                <form class="slides-form center left" action="#" autocomplete="off">
                  <input class="left ae-4" id="full_name" type="text" name="text" placeholder="Full Name" required/>
                  <input class="left ae-4" id="Tile" type="text" name="text" placeholder="Tile" required/>
