<body class="slides zoom horizontal noPreload animated">
  <section class="slide fade-1 kenBurns whiteSlide">
    <div class="content" style="background: #e0f8f6;">
      <div class="container">
        <div class="wrap ">
          <!-- <div class="eqmd-bg" style="z-index:">eqmd</div> -->
          <ul class="fix-12-12">
            <h1 class="huge ae-1 text-cueRed">Data Matters.</h1>
            <p class="hero ae-2">
              Please help us accurately capture
              <a href="#"><u>anoynmous</u></a> <br/> details about who is
              using our tools.
            </p>
            <div class="form-42 margin-top-5">
              <div class="selected left ae-4">
                <form class="slides-form center left" action="#" autocomplete="off">
                  <input class="left ae-4" id="full_name" type="text" name="text" placeholder="Full Name" required/>
                  <input class="left ae-4" id="Tile" type="text" name="text" placeholder="Tile" required/>
                  <input class="left ae-4" id="full_name" type="text" name="text" placeholder="Academic Institution" required/>
                  <input class="left ae-4" id="email42" type="email" name="email" placeholder="Email address" required/>
                  <p class="center ae-7 margin-top-2 micro">
                    <span class="opacity-5">By clicking
                      <a href="#">Create Account</a>
                      , I agree to the<br/>
                      <u><%= link_to "Terms of Servce", login_path, class: "opacity-8 bold" %></u> and
                      <u><%= link_to "Privacy & Policy", login_path, class: "opacity-8 bold" %></u>
                    </span>
                  </p>
                  <%= link_to overview_title_path, class: "button with_icon_right circle btn-primary gradient ae-3" do %>
                    Create Account <i class="far fa-arrow-circle-right"></i>
                  <% end %>
                </form>
              </div>
            </div>
          </ul>
          <br/>
          <br/>
        </div>
      </div>
    </div>
    <div class="background" style="background-image:url();"></div>
