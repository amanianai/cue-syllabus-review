<body class="slides zoom noPreload animated ">
  <style>
     .slides {
        -webkit-transition:1s;
        transition:1s;
     }
     /* set background colors for each of your slides */
     .slides.stage-1 {
        background:#FFFFFF;
     }
     .slides.stage-2 {
        background:#FFFFFF;
     }
     .slides.stage-3 {
        background:#FFFFFF;
     }
     .slides.stage-4 {
        background:#FFFFFF;
     }
     .slides.stage-5 {
        background:#FFFFFF;
     }
     .slides.stage-6 {
        background:#FFFFFF;
     }
     .slides.stage-7 {
        background:#FFFFFF;
     }
     .slides.stage-8 {
        background:#E7FDFF;
     }
     .slides.stage-9 {
        background:#E7FDFF;
     }
     .slides.stage-10 {
        background:#FFFFFF;
     }
     .slides.stage-11 {
        background:#FFFFFF;
     }
     .slides.stage-12 {
        background:#FFFFFF;
     }
     .slide.transparent,
     .slide.transparent .background {
        background:transparent !important;
     }
     .autoHeight {
        min-height:60vh;
     }
     /* ignore */
     .slides.scroll .menuToggle.scrollEffectToggle,
     .slides:not(.scroll) .menuToggle.slideEffectToggle { background: #fff; color:#303030; padding: 11px 7px; margin: -5px 0 !important; border-radius: 5px; }
     .slides.scroll .menuToggle.scrollEffectToggle *,
     .slides:not(.scroll) .menuToggle.slideEffectToggle * { cursor: default !important; opacity:1; pointer-events:none;}}
     /* ignore */
     </style>




