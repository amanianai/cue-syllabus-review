<section class="slide transparent whiteSlide autoHeight font-arial" style="background: #;">
  <div class="content">
    <div class="container">
      <div class="wrap">

        <div class="fix-10-12 left">
          <h1 class="ae-1" style="font-family: 'Arial' !important; font-size: 40px; line-height: 48px;
          text-decoration: underline; text-decoration-color: #2AC940;">Racial equity requires this different <br/>way of looking at success and equity</h1>
        </div>
        <div class="fix-12-12 margin-top-10">
          <ul class="flex fixedSpaces later left">
            <li class="col-2-12 col-tablet-1-2 col-phone-1-1">
              <h4 class="ae-4">Racial identity</h4>
              <%= image_tag("graphics/squares/purple/1.png", class: "margin-top-1 wide shadow ae-1 fromCenter")%>
            </li>
            <li class="col-2-12 col-tablet-1-2 col-phone-1-1">
              <%= image_tag("graphics/squares/purple/2.png", class: "margin-bottom-1 wide shadow ae-1 fromCenter")%>
              <h4 class="ae-5">Racialized patterns</h4>
            </li>
            <li class="col-4-12 col-tablet-1-2 col-phone-1-1" style="position: relative;">
              <%= image_tag("graphics/sections/3/head-vector.png", class: "wide ae-1  fromCenter")%>
              <a href="?example=multipage-1" class="">
                <%= image_tag("graphics/sections/3/orange-squares.png", style: "position: absolute; left: 38%; top: 20%;  height: 40px; width: 40px;", class: "shadow ae-1  fromCenter")%>
              </a>
              <a href="?example=multipage-2" class="">
                <%= image_tag("graphics/sections/3/blue-squares.png", style: "position: absolute; top: 32%;  right: 35%;  height: 40px; width: 40px;", class: "shadow ae-1 fromCenter")%>
              </a>
            </li>
            <li class="col-2-12 col-tablet-1-2 col-phone-1-1">
              <h4 class="ae-6">Racial consequences</h4>
              <%= image_tag("graphics/squares/purple/2.png", class: "margin-top-1 wide shadow ae-1 fromCenter")%>
            </li>
            <li class="col-2-12 col-tablet-1-2 col-phone-1-1">
              <%= image_tag("graphics/squares/purple/2.png", class: "margin-bottom-1 wide shadow ae-1 fromCenter")%>
              <h4 class="ae-7">Producing racial equity</h4>
            </li>
          </ul>
        </div>

      </div>
    </div>
  </div>
</section>
