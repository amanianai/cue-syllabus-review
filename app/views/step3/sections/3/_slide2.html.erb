<section class="slide transparent whiteSlide autoHeight font-arial" style="background: #;">
  <div class="content">
    <div class="container">
      <div class="wrap">

        <div class="fix-10-12 left">
          <h1 class="ae-1" style="font-family: 'Arial' !important; font-size: 40px; line-height: 48px;
          text-decoration: underline; text-decoration-color: #FF5F44;">What if we thought about <br/>student success differently?</h1>
        </div>
        <div class="fix-12-12 margin-top-10">
          <ul class="flex fixedSpaces later left">
            <li class="col-2-12 col-tablet-1-2 col-phone-1-1">
              <h5 class="ae-4">Practitioner effort</h5>
              <%= image_tag("graphics/squares/blue/1.png", class: "wide ae-1 fromCenter")%>
            </li>
            <li class="col-2-12 col-tablet-1-2 col-phone-1-1">
              <%= image_tag("graphics/squares/blue/2.png", class: "wide ae-1 fromCenter")%>
              <h5 class="ae-5">Practitioner care</h5>
            </li>
            <li class="col-4-12 col-tablet-1-2 col-phone-1-1" style="position: relative;">
              <%= image_tag("graphics/sections/3/head-vector.png", class: "wide ae-1  fromCenter")%>
              <%= image_tag("graphics/sections/3/blue-squares.png", style: "position: absolute; right: 40%; top: 30%;  height: 40px; width: 40px;", class: "shadow ae-1  fromCenter")%>
              <%= image_tag("graphics/sections/3/purple-squares.png", style: "position: absolute; bottom: 30%;  right: 30%;  height: 40px; width: 40px;", class: "shadow ae-1 fromCenter")%>
            </li>
            <li class="col-2-12 col-tablet-1-2 col-phone-1-1">
              <h5 class="ae-6">Practitioner responsibility</h5>
              <%= image_tag("graphics/squares/blue/2.png", class: "wide ae-1 fromCenter")%>
            </li>
            <li class="col-2-12 col-tablet-1-2 col-phone-1-1">
              <%= image_tag("graphics/squares/blue/2.png", class: "wide ae-1 fromCenter")%>
              <h5 class="ae-7">Institutional commitment</h5>
            </li>
          </ul>
        </div>

      </div>
    </div>
  </div>
</section>
