      }
      .slides.stage-3 {
         background: lightgray;
      }
      .slides.stage-4 {
         background: yellow;
      }
      .slides.stage-5 {
         background:#489D43;
      }

      .slide.transparent,
      .slide.transparent .background {
         background:transparent !important;
      }

      .autoHeight {
         min-height:60vh;
      }

      /* ignore */
